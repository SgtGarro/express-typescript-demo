{
  "env": {
    "node": true,
    "jest": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "no-var": "error",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
